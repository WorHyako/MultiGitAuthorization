Host first-ssh-host-custom-name
	HostName github.com
	User user-name-for-first-acc
	IdentityFile ~/path/to/first-ssh-private-key
	IdentitiesOnly yes
	PreferredAuthentications publickey
	PasswordAuthentication no

Host second-ssh-host-custom-name
	Port server-port
	HostName www.self-hosted-server-name.org
	User user-name-for-second-acc
	IdentityFile C:/path/to/second-ssh-private-key
	IdentitiesOnly yes
	PreferredAuthentications publickey
	PasswordAuthentication no
