Host github.com
	HostName github.com
	User user-name-for-first-acc
	IdentityFile C:/path/to/first-ssh-key
	IdentitiesOnly yes
	PreferredAuthentications publickey
	PasswordAuthentication no

Host www.self-hosted-server-name.org
	Port server-port
	HostName www.self-hosted-server-name.org
	User user-name-for-second-acc
	IdentityFile C:/path/to/second-ssh-key
	IdentitiesOnly yes
	PreferredAuthentications publickey
	PasswordAuthentication no
